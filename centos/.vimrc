set nocompatible
set encoding=utf-8
set fileencodings=utf-9,cp949
set fileformats=unix,dos
set backup
set backupdir=~/backup
set history=50
set ignorecase
set smartcase
set hlsearch
set incsearch
set number

set list


set showmatch



syntax on


highlight Comment ctermfg=LightCyan


set wrap


set shiftwidth=4




set tabstop=4



set ruler



set autoindent